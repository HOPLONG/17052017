
@{
    ViewBag.Title = "DonBanHangHome";
}

<script src="~/Content/self-writing/js/Controllers/RouteCtrl.js"></script>
<script src="~/Content/self-writing/js/Controllers/BanHangCtrl.js"></script>
<script src="~/Content/self-writing/js/Services/BanHangService.js"></script>
<link href="~/Content/self-writing/css/Kinhdoanh/donhangPO.css" rel="stylesheet" />
<div class="panel-heading">
    <marquee width="80%">Bạn đang xem danh mục đơn bán hàng</marquee>
</div>
<div ng-app="myApp"
     ng-controller="HomeBanHangCtrl"
     layout="column">
    <input type="hidden" value="@Session["USERNAME"]" id="username" />
    <input type="hidden" value="@Session["IS_AMIN"]" id="isadmin" />

    <md-tabs md-stretch-tabs
             md-selected="selectedIndex">
        <md-tab label="List đơn bán hàng"></md-tab>
        <md-tab label="List đơn bán hàng chưa xuất kho"></md-tab>
        <md-tab label="List đơn bán hàng đã xuất kho"></md-tab>
    </md-tabs>

    <div id="content" ui-view flex> </div>

</div>

